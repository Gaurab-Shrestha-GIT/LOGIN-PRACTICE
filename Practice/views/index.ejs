<!-- Main Site -->
<main id="site-main">
  <div class="container">
    <% if (user) { %>

    <a href="/logout"><button>Logout</button></a>

    <div>
      <li>Welcome, <%= user.email %></li>
    </div>

    <% } else { %>
    <a href="/login"> <button type="button btn-primary">Login</button> </a>
    <a href="/signup">
      <button type="button btn btn-primary">SignUp</button>
    </a>
    <% } %>
  </div>
</main>
<!-- Main Site -->
